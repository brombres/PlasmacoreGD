module Plasmacore

class Log : BufferedPrintWriter<<output_buffer>> [singleton]
  NATIVE
    nativeHeader @|#include <godot_cpp/variant/utility_functions.hpp>

  METHODS
    method flush( buffer:String )
      native
      @|godot::UtilityFunctions::print( $buffer->data->as_utf8 );
      buffer.clear
endClass
